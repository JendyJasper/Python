print "Dont enter numbers less than 2. All numbers should be more than 1"

print "Dont enter numbers less than 2. All numbers should be more than 1"

#Uncomment the two lines below to accept input from user

#first_lower = int(raw_input("Enter the lower number here>>>"))
#second_higher = int(raw_input("Enter the higher number here>>>"))

def primer(first_lower, second_higher):
    for num in range(first_lower, second_higher+1, -1):
        if num > 1:
            for i in range(2, num):
                if num == 2:
                    print num
                else:
                    if num % i == 0:
                        break
            else:
                print num
        else:
            print " %d is not greater than 1" % first_lower

